<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.slidingmenudemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabHostActivity"/>
        <activity android:name=".TabActivity.TabActivity1"/>
        <activity android:name=".TabActivity.TabActivity2"/>
        <activity android:name=".TabActivity.TabActivity3"/>
        <activity android:name=".TabActivity.TabActivity4"/>
    </application>

</manifest>